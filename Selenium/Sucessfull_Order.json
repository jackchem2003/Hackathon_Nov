{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://fosimple.azurewebsites.net/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "txtPhone"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "txtPhone"
      },
      "text": "9876543211"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "txtEmail"
      },
      "text": "xyz@verizon.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "txtZip"
      },
      "text": "75001"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "lnkOrder"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Thank you for Ordering FiOS "
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}
